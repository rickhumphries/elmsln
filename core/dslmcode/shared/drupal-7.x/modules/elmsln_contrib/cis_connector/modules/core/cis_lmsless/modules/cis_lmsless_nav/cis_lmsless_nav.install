<?php

/**
 * @file
 * Install/uninstall functions for the CIS LMS-less module.
 */

/**
 * Implements hook_install().
 */
function cis_lmsless_nav_install() {
  variable_set('cis_lmsless_nav_links', array());
}

/**
 * Implements hook_uninstall().
 */
function cis_lmsless_nav_uninstall() {
  variable_del('cis_lmsless_nav_links');
}